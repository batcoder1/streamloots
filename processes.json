{
  "apps": [{
    "name": "streamloots",
    "script": "dist/src/main.js",
    "watch": true,
    "wait_ready": true,
    "instances": "max",
    "exec_mode": "cluster",
    "instance_var": "INSTANCE_ID",
    "listen_timeout": 3000,
    "log_file": "/dev/null",
    "out_file": "/dev/null",
    "error_file": "/dev/null",
    "merge_logs": true,
    "env": {
      "NODE_ENV": "production"
    }
  }]
}
